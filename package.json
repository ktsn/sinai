{
  "name": "sinai",
  "version": "0.3.0",
  "author": "katashin",
  "description": "Type safe state management inspired by Vuex",
  "keywords": [
    "typescript",
    "type safe",
    "state management",
    "vue",
    "vuex"
  ],
  "license": "MIT",
  "main": "./dist/sinai.umd.cjs",
  "module": "./dist/sinai.js",
  "exports": {
    ".": {
      "import": "./dist/sinai.js",
      "require": "./dist/sinai.umd.cjs"
    }
  },
  "types": "types/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "types"
  ],
  "homepage": "https://github.com/ktsn/sinai",
  "bugs": "https://github.com/ktsn/sinai/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/sinai.git"
  },
  "scripts": {
    "example": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "release": "./scripts/release.sh"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@vitejs/plugin-vue2": "^2.3.1",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.1",
    "typescript": "~5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.0.4",
    "vue": "^2.7.15",
    "vue-template-compiler": "^2.7.15"
  }
}
